  "scripts": {
    "build": "nest build",
    "build:cli": "nest build --builder webpack --webpackPath webpack.cli.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prestart": "npm run prisma:generate",
    "start": "nest start --watch",
    "start:dev:docker": "docker compose -f docker-compose.dev.yml --env-file .env.dev up",
    "start:dev:docker:build": "docker compose -f docker-compose.dev.yml --env-file .env.dev up --build",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
